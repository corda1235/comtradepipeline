# Creare un esempio da condividere su GitHub (senza credenziali vere)
cp .env .env.example
# Modificare .env.example per rimuovere credenziali vere
sed -i 's/your_primary_key_here/your_primary_key/g' .env.example
sed -i 's/your_secondary_key_here/your_secondary_key/g' .env.example
sed -i 's/your_password_here/your_password/g' .env.example